{
  "compilerOptions": {
    "module": "es2020",
    "downlevelIteration": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
    // 其他配置...
  },
  "include": [  
    "src/**/*.ts",  
    "src/**/*.tsx",  
    "src/**/*.vue",  
    "tests/**/*.ts",  
    "tests/**/*.tsx"  
  ],
  "exclude": [
    "src/components/AceEditor/aceConfig.js",
    "src/router/index.js"
  ]
}
